<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/common_margin_46dp"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:background="@drawable/selector_bg_white_green">

    <ImageView
        android:visibility="gone"
        android:id="@+id/id_treenode_icon"
        android:layout_width="@dimen/common_margin_30dp"
        android:layout_height="@dimen/common_margin_30dp" />

    <TextView

        android:id="@+id/id_treenode_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2017"
        android:textColor="@color/gray_3333"
        android:textSize="@dimen/textsize_16sp" />

</LinearLayout>